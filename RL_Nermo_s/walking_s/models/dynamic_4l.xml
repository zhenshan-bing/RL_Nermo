<!--        <jointvel name="thigh_joint_fl_vel" joint="thigh_joint_fl"/>-->
<!--        <jointvel name="leg_joint_fl_vel" joint="leg_joint_fl"/>-->
        <!---->
        <!--add sensor for foot touch-->
